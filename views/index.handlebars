
<!--Page Body-->
<main class="page-body">
  <!--start section-->
  <section class="section">
    <!-- start row -->
    <div class="row m-b-0">
      <!-- start column -->
      <div class="col s12 m6">
        <h3 class="header">Admin Aprovals</h3>
        <!--start card-->
        <div class="card m-b-10">
          <!--card content-->
          <div class="card-content">
            <!-- admin approval card -->
            <div id="admin-approval-card" style="height:400px">
              <ul class="dashboard-notifications">
                <li>
                  <a href="#!">
                    <i class="material-icons">sync</i>
                    <p class="noti-message">Shift Change Request:
                      <span>
                        Dan would like to switch with Push
                      </span>
                      <span>
                        Shift: 8/28 from 5pm - 9pm
                      </span>
                    </p>
                  </a>
                </li>
                <li>
                  <a href="#!">
                    <i class="material-icons">access_alarm</i>
                    <p class="noti-message">Time Off Request:
                      <span>
                        Robin requested off
                      </span>
                      <span>
                        dates: 9/1 - 9/5
                      </span>
                      <span>
                         {{title}}
                      </span>
                    </p>
                  </a>
                </li>
              </ul>
            </div>
          </div>
          <!--end card content-->
        </div>
        <!--end card content-->
      </div>
      <!-- end column -->

      <!-- start column -->
      <div class="col s12 m6">
        <h3 class="header">Notifications</h3>
        <!--start card-->
        <div class="card m-b-10">
          <!--card content-->
          <div class="card-content">
            <!-- Notification Card -->
            <div id="notification-card" style="height:400px"></div>
          </div>
          <!--end card content-->
        </div>
        <!--end card content-->
      </div>
      <!-- end column -->
    </div>
    <!-- end row -->

    <!-- start row -->
    <div class="row">
      <!-- start column -->
      <div class="col s12">
        <h3 class="header">This Week's Schedule</h3>
        <!--start card-->
        <div class="card">
          <!--card content-->
          <div class="card-content">
            <!-- This weeks schedule display -->
            <div id="current-weeks-schedule" style="height:600px">
              <input id="range_16" />
              <div class="range-slider">
                <input type="text" class="js-range-slider" value="" />
              </div>
              <div class="extra-controls">
                Placeholder for some buttons to change slider behavior
              </div>
            </div>
          </div>
          <!--end card content-->
        </div>
        <!--end card content-->
      </div>
      <!-- end column -->
    </div>
    <!-- end row -->

  </section>
  <!--end section-->

</main>
<!--End page body-->

<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
